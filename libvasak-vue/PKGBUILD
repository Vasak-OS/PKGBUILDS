# Maintainer: Joaquin (Pato) Decima <jdecima@vasak.net.ar>

pkgname=libvasak-vue
pkgver=3.3.4
pkgrel=1
pkgdesc="Vue.js lib for VasakOS"
arch=('any')
url="https://vuejs.org/"
license=('MIT')
source=(
    "https://unpkg.com/vue@$pkgver/dist/vue.global.js"
    "https://unpkg.com/vue@$pkgver/dist/vue.global.prod.js"
)
md5sums=(
    'fe88512d3a1b37b7785cdf0f573b2be8'
    '05943eaa18110ad3a3c3227050c3f5b5'
)

package() {
    install -d ${pkgdir}/usr/lib/${pkgname}/
    for _jsfile in ./*; do
        install -m0644 "${_jsfile}" "${pkgdir}/usr/lib/${pkgname}/"
    done
}
